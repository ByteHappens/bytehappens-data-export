{
  "name": "bytehappens-data-export",
  "version": "1.0.0",
  "description": "Export data",
  "author": "Ewan \"ScottChak\" BURNS",
  "engines": {
    "node": "8.12.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "nf start",
    "build": "grunt build",
    "watch": "grunt watch",
    "setup": "node -r dotenv/config dist/setup dotenv_config_path=environment/dev/.env",
    "debug": "nf start --procfile ./environment/dev/Procfile",
    "dev": "npm run build && npm run setup && concurrently \"npm run debug\" \"npm run watch\"",
    "tsc": "tsc"
  },
  "dependencies": {
    "cron": "^1.4.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "foreman": "^3.0.1",
    "mongodb": "^3.1.6",
    "winston": "^3.1.0",
    "winston-mongodb": "^4.0.3",
    "winston-telegram": "^1.3.0"
  },
  "devDependencies": {
    "@types/cron": "^1.3.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mongodb": "^3.1.10",
    "concurrently": "^4.0.1",
    "grunt": "^1.0.3",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-ts": "^6.0.0-beta.21",
    "nodemon": "^1.18.4",
    "typescript": "^3.1.6"
  }
}
